{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "plugins": [],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "rules": {
    "semi": ["error", "never"],
    "import/extensions": "off",
    "no-console": "off",
    "arrow-parens": ["error", "as-needed"],
    "no-underscore-dangle": "off",
    "comma-dangle": "off"
  }
}